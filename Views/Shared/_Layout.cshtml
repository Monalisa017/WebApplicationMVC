<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title - Meu Aplicativo ASP.NET MVC 5 </title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-os3vj" />
    @Styles.Render("~/assets/css")

</head>
<body>
    @Html.Partial("_Header");
    
    <div class="container body-content">
        @RenderBody()
        <div class="mt-3"></div>
    </div>
    @Html.Partial("_Footer")


    @Html.Partial("_Scripts")
    @RenderSection("scripts", required: false)
</body>
</html>
